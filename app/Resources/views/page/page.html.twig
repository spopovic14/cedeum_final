{% extends 'base.html.twig' %}

{% block body %}

    {% if page.raw == null %}

        <div class="row">
            <div class="box">
                <div class="col-lg-12">
                    {{ page|localizePage('content')|insertImages|raw }}
                </div>
            </div>
        </div>

    {% endif %}

{% endblock %}

{% block content %}

    {% if page.raw %}

        {{ page|localizePage('content')|insertImages|raw }}

    {% endif %}

{% endblock %}

{% block title %}
    {{ page|localizePage('title') }}
{% endblock %}
